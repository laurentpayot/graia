{
    "cSpell.words": [
        "fantomas",
        "Ionide",
        "paket",
        "remotedev",
        "Xunit"
    ],
    "testExplorer.hideWhen": "noTests",
    "[fsharp]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "Ionide.Ionide-fsharp",
        "editor.inlayHints.enabled": "offUnlessPressed"
    },
    "FSharp.showExplorerOnStartup": false,
    "FSharp.excludeProjectDirectories": [
		".git",
        ".config",
		".paket",
        ".vscode",
		"bin",
		"dist",
		"node_modules",
		"obj",
		"output",
		"paket-files"
    ]
}
